{
  "collection": "@nestjs/schematics",
  "monorepo": true,
  "compilerOptions": {
    "webpack": false
  },
  "projects": {
    "app": {
      "type": "application",
      "root": "apps/app",
      "entryFile": "main",
      "sourceRoot": "apps/app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/app/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/app",
      "entryFile": "authHandler",
      "sourceRoot": "apps/app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/app/tsconfig.app.json"
      }
    },
    "cms": {
      "type": "application",
      "root": "apps/cms",
      "entryFile": "main",
      "sourceRoot": "apps/cms/src",
      "compilerOptions": {
        "tsConfigPath": "apps/cms/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "database": {
      "type": "library",
      "root": "libs/database",
      "entryFile": "index",
      "sourceRoot": "libs/database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database/tsconfig.lib.json"
      }
    },
    "power": {
      "type": "library",
      "root": "libs/power",
      "entryFile": "index",
      "sourceRoot": "libs/power/src",
      "compilerOptions": {
        "tsConfigPath": "libs/power/tsconfig.lib.json"
      }
    },
    "auth-provider": {
      "type": "library",
      "root": "libs/auth-provider",
      "entryFile": "index",
      "sourceRoot": "libs/auth-provider/src",
      "compilerOptions": {
        "tsConfigPath": "libs/auth-provider/tsconfig.lib.json"
      }
    },
    "generate-csv-report": {
      "type": "library",
      "root": "libs/generate-csv-report",
      "entryFile": "index",
      "sourceRoot": "libs/generate-csv-report/src",
      "compilerOptions": {
        "tsConfigPath": "libs/generate-csv-report/tsconfig.lib.json"
      }
    }
  }
}
