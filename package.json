  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "AWS_PROFILE=power AWS_REGION=ap-south-1 npx sls offline",
    "start:sls": "serverless offline --noPrependStageInUrl --allowCache",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "deploy:dev": "npx sls deploy -s development",
    "deploy:qa": "npx sls deploy -s qa",
    "deploy:prod": "npx sls deploy -s prod",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/boilerplate-aws/test/jest-e2e.json",
    "migrate:make:app": "bash db/app/make-migration.sh",
    "migrate:make:user": "bash db/user/make-migration.sh",
    "migrate:app:down": "knex migrate:rollback --knexfile db/app/knexfile.js",
    "migrate:app:up": "knex migrate:latest --knexfile db/app/knexfile.js --env qa",
    "migrate:user:down": "knex migrate:rollback --knexfile db/user/knexfile.js --env development",
    "migrate:user:up": "knex migrate:latest --knexfile db/user/knexfile.js --env qa"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/thedistance/the-core-v4-boilerplate-aws.git"
  }
}
