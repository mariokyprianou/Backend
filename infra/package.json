{
  "name": "infra",
  "version": "0.1.0",
  "bin": {
    "infra": "bin/infra.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "diff:dev": "cdk diff --region ap-south-1 --profile power dev-infra",
    "diff:qa": "cdk diff --region ap-south-1 --profile power qa-infra",
    "diff:uat": "cdk diff --region ap-south-1 --profile power uat-infra",
    "diff:prod": "cdk diff --region ap-south-1 --profile power prod-infra",
    "deploy:dev": "cdk deploy --region ap-south-1 --profile power dev-infra",
    "deploy:qa": "cdk deploy --region ap-south-1 --profile power qa-infra",
    "deploy:uat": "cdk diff --region ap-south-1 --profile power uat-infra",
    "deploy:prod": "cdk diff --region ap-south-1 --profile power prod-infra"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.75.0",
    "@types/jest": "^26.0.10",
    "@types/node": "10.17.27",
    "jest": "^26.4.2",
    "ts-jest": "^26.2.0",
    "aws-cdk": "1.75.0",
    "ts-node": "^9.0.0",
    "typescript": "~3.9.7"
  },
  "dependencies": {
    "@aws-cdk/aws-cognito": "^1.75.0",
    "@aws-cdk/aws-ec2": "^1.75.0",
    "@aws-cdk/aws-rds": "^1.75.0",
    "@aws-cdk/aws-s3": "^1.75.0",
    "@aws-cdk/core": "1.75.0",
    "envalid": "^6.0.2",
    "source-map-support": "^0.5.16"
  }
}